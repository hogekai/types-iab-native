{
  "name": "iab-native",
  "version": "0.5.1",
  "description": "TypeScript definitions fro IAB Tech Lab`s Native Ad",
  "scripts": {
    "build": "tsup"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./v12": {
      "import": "./src/v12/index.ts",
      "require": "./src/v12/index.ts",
      "types": "./src/v12/index.ts"
    }
  },
  "keywords": [
    "Native",
    "IAB Tech Lab",
    "typescript",
    "types",
    "definitions",
    "advertising",
    "programmatic"
  ],
  "author": {
    "name": "Kai Miyamoto",
    "url": "https://github.com/hogekai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hogekai/types-iab-native.git"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./v12": {
        "import": "./dist/v12/index.mjs",
        "require": "./dist/v12/index.js",
        "types": "./dist/v12/index.d.ts"
      }
    }
  }
}
